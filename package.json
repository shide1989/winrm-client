{
  "name": "winrm-client",
  "version": "0.0.1",
  "description": "Make WinRM service calls from NodeJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "node ./test/test.js",
    "outdated": "npm outdated --json || true",
    "lint": "npx eslint . --ext .ts,.js",
    "dev": "npm run build && npm run lint && npm run format",
    "jest:test": "jest",
    "jest:coverage": "npx jest --coverage",
    "jest:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shide1989/nodejs-winrm.git"
  },
  "keywords": [
    "winrm"
  ],
  "author": "Sebastien Hideux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shide1989/nodejs-winrm/issues"
  },
  "homepage": "https://github.com/shide1989/nodejs-winrm#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "automock": false,
    "coveragePathIgnorePatterns": [],
    "verbose": true,
    "coverageReporters": [
      "html",
      "lcov",
      "json",
      "text",
      "text-summary"
    ],
    "testMatch": [
      "<rootDir>/test/*.unit.spec.js",
      "<rootDir>/__tests__/*.unit.spec.js"
    ]
  },
  "dependencies": {
    "uuid": "^11.1.0",
    "js2xmlparser": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^9.33.0",
    "jest": "^30.0.5",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "@types/node": "^22.8.1",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/parser": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "eslint-config-prettier": "^9.1.0"
  }
}
